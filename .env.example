# GraphDB Configuration
GRAPHDB_HOST=localhost
GRAPHDB_PORT=7200
GRAPHDB_USERNAME=admin
GRAPHDB_PASSWORD=admin
GRAPHDB_REPOSITORY=vietnamese-dbpedia

# Wikipedia API Configuration
WIKIPEDIA_USER_AGENT=Vietnamese-DBPedia-Extractor/1.0
WIKIPEDIA_RATE_LIMIT=1
WIKIPEDIA_MAX_ARTICLES=1000

# Application Configuration
DEBUG=true
LOG_LEVEL=DEBUG
DATA_DIR=./data
CONFIG_DIR=./config

# Web Interface Configuration
FLASK_APP=src/interfaces/web_interface.py
FLASK_ENV=development
FLASK_PORT=5000
FLASK_HOST=0.0.0.0

# Entity Linking Configuration
DBPEDIA_SPARQL_ENDPOINT=https://dbpedia.org/sparql
ENTITY_LINKING_THRESHOLD=0.8
MAX_SIMILARITY_CANDIDATES=10

# Performance Configuration
BATCH_SIZE=50
CONCURRENT_REQUESTS=3
REQUEST_TIMEOUT=30
QUERY_TIMEOUT=60

# Backup Configuration
BACKUP_DIR=./backups
BACKUP_RETENTION_DAYS=30