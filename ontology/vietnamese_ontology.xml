<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
>
  <rdf:Description rdf:about="http://vi.dbpedia.org/property/triềuĐại">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="vi">triều đại</rdfs:label>
    <rdfs:label xml:lang="en">dynasty period</rdfs:label>
    <rdfs:comment xml:lang="vi">Thời kỳ triều đại</rdfs:comment>
    <rdfs:domain rdf:resource="http://vi.dbpedia.org/ontology/Người"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/NhânVậtChínhTrị">
    <rdfs:subClassOf rdf:resource="http://vi.dbpedia.org/ontology/Người"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Nhân vật chính trị</rdfs:label>
    <rdfs:label xml:lang="en">Political Figure</rdfs:label>
    <rdfs:comment xml:lang="vi">Người tham gia hoạt động chính trị</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Politician"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/property/nơiSinh">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="vi">nơi sinh</rdfs:label>
    <rdfs:label xml:lang="en">birth place</rdfs:label>
    <rdfs:comment xml:lang="vi">Nơi sinh của một người</rdfs:comment>
    <rdfs:domain rdf:resource="http://vi.dbpedia.org/ontology/Người"/>
    <rdfs:range rdf:resource="http://vi.dbpedia.org/ontology/ĐịaĐiểm"/>
    <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/birthPlace"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/NghệSĩ">
    <rdfs:subClassOf rdf:resource="http://vi.dbpedia.org/ontology/Người"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Nghệ sĩ</rdfs:label>
    <rdfs:label xml:lang="en">Artist</rdfs:label>
    <rdfs:comment xml:lang="vi">Người hoạt động trong lĩnh vực nghệ thuật</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Artist"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/property/ngàySinh">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label xml:lang="vi">ngày sinh</rdfs:label>
    <rdfs:label xml:lang="en">birth date</rdfs:label>
    <rdfs:comment xml:lang="vi">Ngày sinh của một người</rdfs:comment>
    <rdfs:domain rdf:resource="http://vi.dbpedia.org/ontology/Người"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
    <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/birthDate"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/Người">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Người</rdfs:label>
    <rdfs:label xml:lang="en">Person</rdfs:label>
    <rdfs:comment xml:lang="vi">Lớp đại diện cho tất cả con người</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Person"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/SựKiện">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Sự kiện</rdfs:label>
    <rdfs:label xml:lang="en">Event</rdfs:label>
    <rdfs:comment xml:lang="vi">Sự kiện, biến cố lịch sử hoặc văn hóa</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Event"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/TácPhẩm">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Tác phẩm</rdfs:label>
    <rdfs:label xml:lang="en">Work</rdfs:label>
    <rdfs:comment xml:lang="vi">Tác phẩm văn học, nghệ thuật</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Work"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/TổChức">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Tổ chức</rdfs:label>
    <rdfs:label xml:lang="en">Organization</rdfs:label>
    <rdfs:comment xml:lang="vi">Tổ chức, cơ quan, đơn vị</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Organisation"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <rdfs:label xml:lang="en">Vietnamese DBPedia Ontology</rdfs:label>
    <rdfs:label xml:lang="vi">Bản thể học DBPedia Việt Nam</rdfs:label>
    <rdfs:comment xml:lang="en">Comprehensive ontology for Vietnamese knowledge representation</rdfs:comment>
    <rdfs:comment xml:lang="vi">Bản thể học toàn diện cho biểu diễn tri thức Việt Nam</rdfs:comment>
    <dct:creator>Vietnamese DBPedia Project</dct:creator>
    <owl:versionInfo>1.0</owl:versionInfo>
    <owl:imports rdf:resource="http://dbpedia.org/ontology/"/>
    <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/property/tọaLạcTại">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="vi">tọa lạc tại</rdfs:label>
    <rdfs:label xml:lang="en">located in</rdfs:label>
    <rdfs:comment xml:lang="vi">Vị trí địa lý của một địa điểm</rdfs:comment>
    <rdfs:domain rdf:resource="http://vi.dbpedia.org/ontology/ĐịaĐiểm"/>
    <rdfs:range rdf:resource="http://vi.dbpedia.org/ontology/ĐịaĐiểm"/>
    <owl:equivalentProperty rdf:resource="http://dbpedia.org/ontology/location"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/ontology/ĐịaĐiểm">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="vi">Địa điểm</rdfs:label>
    <rdfs:label xml:lang="en">Place</rdfs:label>
    <rdfs:comment xml:lang="vi">Vị trí địa lý cụ thể</rdfs:comment>
    <owl:equivalentClass rdf:resource="http://dbpedia.org/ontology/Place"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://vi.dbpedia.org/property/trungTâmHànhChính">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="vi">trung tâm hành chính</rdfs:label>
    <rdfs:label xml:lang="en">administrative center</rdfs:label>
    <rdfs:comment xml:lang="vi">Trung tâm hành chính của vùng</rdfs:comment>
    <rdfs:domain rdf:resource="http://vi.dbpedia.org/ontology/ĐịaĐiểm"/>
    <rdfs:range rdf:resource="http://vi.dbpedia.org/ontology/ĐịaĐiểm"/>
  </rdf:Description>
</rdf:RDF>
